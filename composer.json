{
    "name": "denisparamon/difrences-files",
    "bin": [
        "bin/gendiff"
    ],
    "autoload": {
        "files": [
            "src/Differ.php",
            "src/Parsers.php",
            "src/Formatters.php",
            "src/Formatters/Json.php",
            "src/Formatters/Plain.php",
            "src/Formatters/Stylish.php"
        ],
        "psr-4": {
            "Differ\\Tests\\": "tests",
            "Differ\\": "src"
        }
    },
    "description": "Generate diff",
    "type": "project",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Hooman",
            "email": "denis_paramon@icloud.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "docopt/docopt": "^1.0",
        "symfony/yaml": "^5.2",
        "funct/funct": "^1.5"
    },
    "scripts": {
        "phpcs": "phpcs",
        "phpunit": "phpunit"
    },
    "license": "MIT",
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.11",
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1.11"
    }
}
